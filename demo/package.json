{
  "name": "@angular-material-extensions/calendar-demo",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Demo app for @angular-material-extensions/calendar",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:client-and-server-bundles": "ng build --prod &&  ng run calendar-demo:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate:prerender": "cd dist && node prerender",
    "postinstall": "npm run link",
    "link": "npm link @angular-material-extensions/calendar",
    "start": "ng serve",
    "serve:prod": "ng serve --prod",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/platform-server": "^7.2.8",
    "@angular/router": "^7.2.8",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "@stackblitz/sdk": "^1.3.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.7",
    "@angular-devkit/core": "^7.3.5",
    "@angular-devkit/schematics": "^7.3.5",
    "@angular/cli": "^8.1.0",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@angularclass/hmr": "~2.1.3",
    "@compodoc/gulp-compodoc": "^0.0.9",
    "@nguniversal/common": "^7.1.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@types/jest": "^23.3.14",
    "@types/node": "~10.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^4.5.0",
    "express": "^4.16.4",
    "http-server": "^0.11.1",
    "jest": "^23.6.0",
    "protractor": "~5.4.1",
    "reflect-metadata": "~0.1.10",
    "ts-loader": "^5.3.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "~1.7.2",
    "yargs": "^12.0.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  }
}
